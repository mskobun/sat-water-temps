{
  "name": "sat-water-temps",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "lint": "tsc --noEmit --project functions/tsconfig.json",
    "wrangler:dev": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2025-12-11",
    "wrangler:dev:remote": "npm run build && wrangler pages dev .svelte-kit/cloudflare --remote --compatibility-date=2025-12-11",
    "db:export": "wrangler d1 export sat-water-temps-db --remote --output=seed.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1",
    "db:seed": "npm run db:reset && wrangler d1 execute sat-water-temps-db --local --file=seed.sql",
    "db:migrate:local": "wrangler d1 migrations apply sat-water-temps-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply sat-water-temps-db --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.6",
    "svelte": "^5.45.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3",
    "vite": "^7.2.7",
    "wrangler": "^4.53.0"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "maplibre-gl": "^5.0.1",
    "papaparse": "^5.5.3",
    "svelte-maplibre-gl": "^1.0.2"
  }
}
